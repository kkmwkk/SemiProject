<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>jspboard_member</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet-mapping>
  	<servlet-name>MemberFrontController</servlet-name>
  	<url-pattern>*.me</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>GMemberFrontController</servlet-name>
  	<url-pattern>*.gme</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
  	<servlet-name>BoardFrontController</servlet-name>
  	<url-pattern>*.bo</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
  	<servlet-name>AdminFrontController</servlet-name>
  	<url-pattern>*.ad</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<display-name>MemberFrontController</display-name>
  	<servlet-name>MemberFrontController</servlet-name>
  	<servlet-class>com.member.action.MemberFrontController</servlet-class>
  </servlet>
  
  
  <servlet>
  	<display-name>BoardFrontController</display-name>
  	<servlet-name>BoardFrontController</servlet-name>
  	<servlet-class>com.board.action.BoardFrontController</servlet-class>
  </servlet>
  
  
  <servlet>
  	<display-name>GMemberFrontController</display-name>
  	<servlet-name>GMemberFrontController</servlet-name>
  	<servlet-class>com.gmember.action.GMemberFrontController</servlet-class>
  </servlet>
  
  <servlet>
  	<display-name>AdminFrontController</display-name>
  	<servlet-name>AdminFrontController</servlet-name>
  	<servlet-class>com.admin.action.AdminFrontController</servlet-class>
  </servlet>
  
</web-app>